    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flet Debugger: Current File", 
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/Scripts/flet.exe", 
            "args": ["run", "${file}", "-d"], //Web: "--web", "--port", "8080"
            "console": "integratedTerminal"
        }
    ]
}